{
    "org": "$HZN_ORG_ID",
    "label": "$SERVICE_NAME for $ARCH",
    "public": true,
    "documentation": "https://github.com/open-horizon-services/service-fledge",
    "url": "$SERVICE_NAME",
    "version": "$SERVICE_VERSION",
    "arch": "$ARCH",
    "sharable": "singleton",
    "requiredServices": [],
    "userInput": [],
    "deployment": {
        "services": {
            "$SERVICE_NAME": {
                "image": "${DOCKER_IMAGE_BASE}_$ARCH:$DOCKER_IMAGE_VERSION",
                "privileged": false,
                "ports": [
                    {
                        "HostPort": "5200:5200/tcp",
                        "HostIP": "0.0.0.0"
                    }
                ]
            }
        }
    }
}